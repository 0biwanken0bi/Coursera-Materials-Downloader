{
    "manifest_version": 2,

    "name": "Coursera Resource Downloader",
    "description": "This extension downloads coursera's course resource (e.g. video)",
    "version": "1.0",
    "background": {
        "scripts": ["lib/background.js"]
    },
    "browser_action": {
        "default_icon": "images/icon_32.png",
        "default_title": "Download Course's Resource"
    },
    "permissions": [
        "tabs",
        "downloads",
        "http://*/*",
        "https://*/*"
    ]
}
